{
    "ip": "192.168.10.10",
    "memory": 2048,
    "cpus": 1,
    "hostname": "librarysystem",
    "name": "librarysystem",
    "provider": "virtualbox",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": [
        "~/.ssh/id_rsa"
    ],
    "folders": [
        {
            "map": "~/Development/LibrarySystem",
            "to": "/home/vagrant/code"
        }
    ],
    "sites": [
        {
            "map": "homestead.test",
            "to": "/home/vagrant/code/public"
        }
    ],
    "databases": [
        "library"
    ]
}
